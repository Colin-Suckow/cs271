/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Or(a=a, b=b, out=outor);
    And(a=a, b=b, out=equal);
    Not(in=equal, out=notequal);
    And(a=outor, b=notequal, out=out);
}
